            }
          },
          "204": {
            "description": "Returns an empty object signaling that the user no longer exists",
            "schema": {
              "$ref": "#/noContent"
            }
          },
          "500": {
            "description": "Returns the message stating that an error has occurred on the server",
            "schema": {
              "$ref": "#/internalServerError"
            }
          }
        }
      }
    },
